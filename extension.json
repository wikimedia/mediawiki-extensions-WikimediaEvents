				"index.js",
				"events.js",
				"statsd.js",
				"deprecate.js",
				"clientError.js",
				"sessionTick.js",
				{
					"name": "searchSatisfaction.js",
					"callback": "WikimediaEvents\\WikimediaEventsHooks::getModuleFile",
					"callbackParam": "searchSatisfaction"
				},
				{
					"name": "desktopWebUIActions.js",
					"callback": "WikimediaEvents\\WikimediaEventsHooks::getModuleFile",
					"callbackParam": "desktopWebUIActions"
