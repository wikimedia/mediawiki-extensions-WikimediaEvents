{
	"root": true,
	"extends": [
		"wikimedia/client-es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"no-var": "warn",
		"compat/compat": "warn",
		"camelcase": [
			"error",
			{ "properties": "never" }
		]
	},
	"env": {
		"commonjs": true
	},
	"settings": {
		"polyfills": [
			"URL"
		]
	}
}
