{
	"root": true,
	"extends": [
		"wikimedia/client-es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"no-var": "warn",
		"compat/compat": "warn",
		"camelcase": [
			"error",
			{
				"properties": "never"
			}
		],
		"no-jquery/no-extend": "warn"
	},
	"env": {
		"commonjs": true
	},
	"settings": {
		"polyfills": [
			"URL"
		]
	}
}
